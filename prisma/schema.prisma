generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model countStatus {
  channelID     String   @id @db.VarChar
  serverID      String   @db.VarChar
  currentNum    BigInt
  highestStreak BigInt
  createdOn     DateTime @db.Timestamptz(6)
  updatedOn     DateTime @db.Timestamptz(6)
}
